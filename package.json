{
  "name": "bankwebapp_backend",
  "version": "1.0.0",
  "description": "A simple online bank web application APIs",
  "main": "index.js",
  "type": "module",
  "repository": "https://github.com/abfatahi/bankwebapp_backend.git",
  "author": "abfatahi <abfatahi.iaf@gmail.com>",
  "license": "MIT",
