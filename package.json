    "v-response": "^1.1.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "restartable": "yes",
    "ignore": [
      "node_modules/**/node_modules"
